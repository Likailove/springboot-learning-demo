        <!--"http://mybatis.org/dtd/mybatis-3-config.dtd">-->
<!--<configuration>-->
    <!--&lt;!&ndash;<settings>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 使全局的映射器启用或禁用缓存。 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="cacheEnabled" value="true" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 全局启用或禁用延迟加载。当禁用时，所有关联对象都会即时加载。 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="lazyLoadingEnabled" value="true" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 当启用时，有延迟加载属性的对象在被调用时将会完全加载任意属性。否则，每种属性将会按需要加载。 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="aggressiveLazyLoading" value="true"/>&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 是否允许单条sql 返回多个数据集  (取决于驱动的兼容性) default:true &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="multipleResultSetsEnabled" value="true" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 是否可以使用列的别名 (取决于驱动的兼容性) default:true &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="useColumnLabel" value="true" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 允许JDBC 生成主键。需要驱动器支持。如果设为了true，这个设置将强制使用被生成的主键，有一些驱动器不兼容不过仍然可以执行。  default:false  &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="useGeneratedKeys" value="false" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 指定 MyBatis 如何自动映射 数据基表的列 NONE：不隐射　PARTIAL:部分  FULL:全部  &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="autoMappingBehavior" value="PARTIAL" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 这是默认的执行类型  （SIMPLE: 简单； REUSE: 执行器可能重复使用prepared statements语句；BATCH: 执行器可以重复执行语句和批量更新）  &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="defaultExecutorType" value="SIMPLE" />&ndash;&gt;-->

        <!--&lt;!&ndash;<setting name="defaultStatementTimeout" value="25" />&ndash;&gt;-->

        <!--&lt;!&ndash;<setting name="defaultFetchSize" value="100" />&ndash;&gt;-->

        <!--&lt;!&ndash;<setting name="safeRowBoundsEnabled" value="false" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 使用驼峰命名法转换字段。 &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="mapUnderscoreToCamelCase" value="true" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 设置本地缓存范围 session:就会有数据的共享  statement:语句范围 (这样就不会有数据的共享 ) defalut:session &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="localCacheScope" value="SESSION" />&ndash;&gt;-->
        <!--&lt;!&ndash;&lt;!&ndash; 默认为OTHER,为了解决oracle插入null报错的问题要设置为NULL &ndash;&gt;&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="jdbcTypeForNull" value="NULL" />&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString" />&ndash;&gt;-->

        <!--&lt;!&ndash;指定MyBatis的日志实现使用。如果此设置是不存在的记录的实施将自动查找。&ndash;&gt;-->
        <!--&lt;!&ndash;<setting name="logImpl" value="LOG4J2"/>&ndash;&gt;-->

